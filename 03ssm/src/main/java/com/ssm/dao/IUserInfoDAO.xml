<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.dao.IUserInfoDAO">
    <select id="isLogin" resultType="UserInfo">
        select username,address from userinfo where username=#{username} and userpwd=#{userpwd}
    </select>
</mapper>